{
    "name": "tmc-frontend",
    "version": "0.1.0",
    "private": true,
    "main": "main.js",
    "author": "Track My Call",
    "homepage": "./",
    "description": "Track My Call - Desktop Application for call management and prospecting",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "electron-start": "npm run build && electron .",
        "electron-build": "npm run build && electron-builder",
        "electron-pack": "electron-builder"
    },
    "dependencies": {
        "@abandonware/noble": "^1.9.2-25",
        "@auth0/auth0-react": "^2.3.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@ffmpeg/core": "^0.12.10",
        "@fortawesome/fontawesome-svg-core": "^6.7.2",
        "@fortawesome/free-brands-svg-icons": "^6.6.0",
        "@fortawesome/free-regular-svg-icons": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "@mui/icons-material": "^5.16.6",
        "@mui/joy": "^5.0.0-beta.48",
        "@mui/x-date-pickers": "^7.16.0",
        "@reduxjs/toolkit": "^2.2.7",
        "@tanstack/react-query": "^5.51.23",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.103",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "audiobuffer-to-wav": "^1.0.0",
        "axios": "^1.7.3",
        "dayjs": "^1.11.13",
        "gsap": "^3.12.5",
        "html-react-parser": "^5.1.15",
        "joi": "^17.13.3",
        "path-browserify": "^1.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-speech-to-text": "^0.8.0",
        "react-icons": "^5.5.0",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.26.0",
        "react-scripts": "5.0.1",
        "tone": "^15.0.4",
        "typescript": "^4.9.5",
        "wav-encoder": "^1.3.0",
        "waves-audio": "github:wavesjs/waves-audio",
        "wavesurfer.js": "^7.9.1",
        "web-vitals": "^2.1.4"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/audiobuffer-to-wav": "^1.0.4",
        "@types/p5": "^1.7.6",
        "@types/wav-encoder": "^1.3.3",
        "electron": "^33.3.1",
        "electron-builder": "^25.1.8",
        "electron-is-dev": "^3.0.1",
        "electron-reload": "^2.0.0-alpha.1",
        "prettier": "^3.5.3"
    },
    "build": {
        "icon": "public/icon/logo1.png",
        "appId": "com.tmc.app",
        "productName": "TrackMyCall",
        "files": [
            "build/**/*",
            "main.js"
        ],
        "directories": {
            "output": "dist",
            "buildResources": "assets"
        },
        "mac": {
            "icon": "public/logo/logo1.png",
            "category": "public.app-category.utilities",
            "target": [
                "dmg",
                "zip"
            ]
        },
        "win": {
            "icon": "assets/icon.ico",
            "target": "nsis"
        },
        "linux": {
            "icon": "assets/icon.png",
            "target": "AppImage"
        }
    }
}
